{
  "groups": [
    {
      "name": "omni-agent.rag.mongodb",
      "type": "top.yumbo.ai.rag.mongodb.MongoDBRAGProperties",
      "sourceType": "top.yumbo.ai.rag.mongodb.MongoDBRAGProperties",
      "description": "MongoDB RAG配置属性"
    }
  ],
  "properties": [
    {
      "name": "omni-agent.rag.type",
      "type": "java.lang.String",
      "description": "RAG实现类型(file, sqlite, mongodb等)",
      "defaultValue": "file"
    },
    {
      "name": "omni-agent.rag.mongodb.uri",
      "type": "java.lang.String",
      "description": "MongoDB连接URI",
      "defaultValue": "mongodb://localhost:27017"
    },
    {
      "name": "omni-agent.rag.mongodb.database",
      "type": "java.lang.String",
      "description": "数据库名称",
      "defaultValue": "omni_rag"
    },
    {
      "name": "omni-agent.rag.mongodb.collection-name",
      "type": "java.lang.String",
      "description": "集合名称",
      "defaultValue": "rag_documents"
    },
    {
      "name": "omni-agent.rag.mongodb.enable-text-search",
      "type": "java.lang.Boolean",
      "description": "启用文本搜索",
      "defaultValue": true
    },
    {
      "name": "omni-agent.rag.mongodb.connection-timeout",
      "type": "java.lang.Integer",
      "description": "连接超时(毫秒)",
      "defaultValue": 10000
    },
    {
      "name": "omni-agent.rag.mongodb.socket-timeout",
      "type": "java.lang.Integer",
      "description": "Socket超时(毫秒)",
      "defaultValue": 30000
    },
    {
      "name": "omni-agent.rag.mongodb.max-pool-size",
      "type": "java.lang.Integer",
      "description": "最大连接池大小",
      "defaultValue": 100
    },
    {
      "name": "omni-agent.rag.mongodb.min-pool-size",
      "type": "java.lang.Integer",
      "description": "最小连接池大小",
      "defaultValue": 10
    },
    {
      "name": "omni-agent.rag.mongodb.server-selection-timeout",
      "type": "java.lang.Integer",
      "description": "服务器选择超时(毫秒)",
      "defaultValue": 30000
    }
  ]
}
