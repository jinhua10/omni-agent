{
  "groups": [
    {
      "name": "omni-agent.rag.redis",
      "type": "top.yumbo.ai.rag.redis.RedisRAGProperties",
      "sourceType": "top.yumbo.ai.rag.redis.RedisRAGProperties",
      "description": "Redis RAG配置"
    }
  ],
  "properties": [
    {
      "name": "omni-agent.rag.redis.key-prefix",
      "type": "java.lang.String",
      "description": "Redis Key前缀",
      "defaultValue": "rag:"
    },
    {
      "name": "omni-agent.rag.redis.enable-text-index",
      "type": "java.lang.Boolean",
      "description": "是否启用文本索引",
      "defaultValue": true
    },
    {
      "name": "omni-agent.rag.redis.document-ttl",
      "type": "java.lang.Long",
      "description": "文档TTL（秒），0表示永不过期",
      "defaultValue": 0
    },
    {
      "name": "omni-agent.rag.redis.connection-timeout",
      "type": "java.lang.Integer",
      "description": "Redis连接超时（毫秒）",
      "defaultValue": 2000
    },
    {
      "name": "omni-agent.rag.redis.read-timeout",
      "type": "java.lang.Integer",
      "description": "Redis读取超时（毫秒）",
      "defaultValue": 5000
    }
  ]
}
