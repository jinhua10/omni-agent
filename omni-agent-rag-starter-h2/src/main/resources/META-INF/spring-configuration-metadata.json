{
  "groups": [
    {
      "name": "omni-agent.rag.h2",
      "type": "top.yumbo.ai.rag.h2.H2RAGProperties",
      "sourceType": "top.yumbo.ai.rag.h2.H2RAGProperties",
      "description": "H2 RAG配置"
    }
  ],
  "properties": [
    {
      "name": "omni-agent.rag.h2.url",
      "type": "java.lang.String",
      "description": "H2数据库URL",
      "defaultValue": "jdbc:h2:./data/omni-agent-rag;AUTO_SERVER=TRUE"
    },
    {
      "name": "omni-agent.rag.h2.username",
      "type": "java.lang.String",
      "description": "数据库用户名",
      "defaultValue": "sa"
    },
    {
      "name": "omni-agent.rag.h2.password",
      "type": "java.lang.String",
      "description": "数据库密码",
      "defaultValue": ""
    },
    {
      "name": "omni-agent.rag.h2.max-pool-size",
      "type": "java.lang.Integer",
      "description": "最大连接池大小",
      "defaultValue": 10
    },
    {
      "name": "omni-agent.rag.h2.min-pool-size",
      "type": "java.lang.Integer",
      "description": "最小空闲连接数",
      "defaultValue": 2
    },
    {
      "name": "omni-agent.rag.h2.connection-timeout",
      "type": "java.lang.Long",
      "description": "连接超时时间（毫秒）",
      "defaultValue": 30000
    },
    {
      "name": "omni-agent.rag.h2.idle-timeout",
      "type": "java.lang.Long",
      "description": "空闲超时时间（毫秒）",
      "defaultValue": 600000
    },
    {
      "name": "omni-agent.rag.h2.max-lifetime",
      "type": "java.lang.Long",
      "description": "连接最大生命周期（毫秒）",
      "defaultValue": 1800000
    }
  ]
}
