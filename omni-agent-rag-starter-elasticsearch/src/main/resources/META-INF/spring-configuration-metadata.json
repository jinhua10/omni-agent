{
  "groups": [
    {
      "name": "omni-agent.rag.elasticsearch",
      "type": "top.yumbo.ai.rag.elasticsearch.ElasticsearchRAGProperties",
      "sourceType": "top.yumbo.ai.rag.elasticsearch.ElasticsearchRAGProperties",
      "description": "Elasticsearch RAG配置"
    }
  ],
  "properties": [
    {
      "name": "omni-agent.rag.elasticsearch.index-name",
      "type": "java.lang.String",
      "description": "索引名称",
      "defaultValue": "omni-agent-rag"
    },
    {
      "name": "omni-agent.rag.elasticsearch.number-of-shards",
      "type": "java.lang.Integer",
      "description": "分片数量",
      "defaultValue": 3
    },
    {
      "name": "omni-agent.rag.elasticsearch.number-of-replicas",
      "type": "java.lang.Integer",
      "description": "副本数量",
      "defaultValue": 1
    },
    {
      "name": "omni-agent.rag.elasticsearch.vector-dimension",
      "type": "java.lang.Integer",
      "description": "向量维度",
      "defaultValue": 768
    },
    {
      "name": "omni-agent.rag.elasticsearch.refresh-after-write",
      "type": "java.lang.Boolean",
      "description": "写入后是否立即刷新",
      "defaultValue": false
    },
    {
      "name": "omni-agent.rag.elasticsearch.connection-timeout",
      "type": "java.lang.Integer",
      "description": "连接超时（毫秒）",
      "defaultValue": 5000
    },
    {
      "name": "omni-agent.rag.elasticsearch.socket-timeout",
      "type": "java.lang.Integer",
      "description": "Socket超时（毫秒）",
      "defaultValue": 60000
    }
  ]
}
